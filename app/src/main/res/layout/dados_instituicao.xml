<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/lblDadosInstituicao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Dados Instituição"
        android:gravity="center"
        android:textSize="20dp"
        android:textColor="@color/colorBlueDark"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" >

    </androidx.appcompat.widget.AppCompatTextView>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/lblRazaoSocial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Razão Social:"
        app:layout_constraintTop_toBottomOf="@id/lblDadosInstituicao"
        tools:ignore="MissingConstraints" >

    </androidx.appcompat.widget.AppCompatTextView>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/lblNomeFantasia"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Nome Fantasia"
    android:layout_marginTop="4dp"
    app:layout_constraintTop_toBottomOf="@id/lblRazaoSocial"
    tools:ignore="MissingConstraints">
</androidx.appcompat.widget.AppCompatTextView>

    <androidx.appcompat.widget.AppCompatTextView
    android:id="@+id/lblCnpj"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="CNPJ"
    android:layout_marginTop="4dp"
    app:layout_constraintTop_toBottomOf="@id/lblNomeFantasia"
    tools:ignore="MissingConstraints">
</androidx.appcompat.widget.AppCompatTextView>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/lblTelefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefone"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@id/lblCnpj"
        tools:ignore="MissingConstraints">
    </androidx.appcompat.widget.AppCompatTextView>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/lblEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="email"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@id/lblTelefone"
        tools:ignore="MissingConstraints">
    </androidx.appcompat.widget.AppCompatTextView>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/lblRua"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rua"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@id/lblEmail"
        tools:ignore="MissingConstraints">
    </androidx.appcompat.widget.AppCompatTextView>





    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.30" />

</androidx.constraintlayout.widget.ConstraintLayout>