        <activity android:name=".view.ListContaActivity" />
        <activity android:name=".view.IncialActivityInsti" />
        <activity android:name=".view.CadastrarInstituicaoActivity" />
        <activity android:name=".view.AberturaActivity" />
        <activity android:name=".view.Cadastrar" />
        <activity android:name=".view.MainActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".view.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>