        android:roundIcon="@drawable/iconelove"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".view.InicialActivityApoiador"></activity>
        <activity android:name=".view.CadastrarConta" />
